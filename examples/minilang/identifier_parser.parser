export module minilang.parser.identifier;

[implementation]import minilang.tree;
[implementation]import minilang.lexer;
[implementation]import minilang.token;

parser IdentifierParser
{
    lexer minilang::lexer::MinilangLexer<char32_t>;

    Identifier : minilang::tree::IdentifierNode*
        ::= ID{ auto token = lexer.GetToken(pos); return new minilang::tree::IdentifierNode(token.ToString()); }
        ;
}
