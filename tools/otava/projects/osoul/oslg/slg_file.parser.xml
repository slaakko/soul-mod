<parserFile>
 <grammar name="SlgFileParser">
  <rule first="{PROJECT}" name="SlgFile">
   <action first="{PROJECT}">
    <group first="{PROJECT}">
     <sequence first="{PROJECT}">
      <sequence first="{PROJECT}">
       <sequence first="{PROJECT}">
        <token first="{PROJECT}" name="PROJECT"/>
        <action first="{ID}">
         <nonterminal first="{ID}" ruleName="QualifiedId"/>
        </action>
       </sequence>
       <token first="{SEMICOLON}" name="SEMICOLON"/>
      </sequence>
      <kleene first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}">
       <group first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}">
        <action first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}">
         <nonterminal first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}" ruleName="SlgFileDeclaration"/>
        </action>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}" name="SlgFileDeclaration">
   <choice first="{#, EXTERN, ID, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}">
    <choice first="{#, EXTERN, SEMICOLON, EXPRESSIONS, FILEPATH, TOKENS, KEYWORDS}">
     <choice first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS, KEYWORDS}">
      <action first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}">
       <nonterminal first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}" ruleName="TokenFileDeclaration"/>
      </action>
      <action first="{KEYWORDS}">
       <nonterminal first="{KEYWORDS}" ruleName="KeywordFileDeclaration"/>
      </action>
     </choice>
     <action first="{EXPRESSIONS}">
      <nonterminal first="{EXPRESSIONS}" ruleName="ExpressionFileDeclaration"/>
     </action>
    </choice>
    <action first="{ID}">
     <nonterminal first="{ID}" ruleName="LexerFileDeclaration"/>
    </action>
   </choice>
  </rule>
  <rule first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}" name="TokenFileDeclaration">
   <action first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}">
    <group first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}">
     <sequence first="{#, EXTERN, SEMICOLON, FILEPATH, TOKENS}">
      <sequence first="{#, EXTERN, FILEPATH, TOKENS}">
       <sequence first="{#, EXTERN, TOKENS}">
        <group first="{#, EXTERN}">
         <choice first="{#, EXTERN}">
          <action first="{EXTERN}">
           <token first="{EXTERN}" name="EXTERN"/>
          </action>
          <action first="{#}">
           <empty first="{#}"/>
          </action>
         </choice>
        </group>
        <token first="{TOKENS}" name="TOKENS"/>
       </sequence>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{KEYWORDS}" name="KeywordFileDeclaration">
   <action first="{KEYWORDS}">
    <group first="{KEYWORDS}">
     <sequence first="{KEYWORDS}">
      <sequence first="{KEYWORDS}">
       <token first="{KEYWORDS}" name="KEYWORDS"/>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{EXPRESSIONS}" name="ExpressionFileDeclaration">
   <action first="{EXPRESSIONS}">
    <group first="{EXPRESSIONS}">
     <sequence first="{EXPRESSIONS}">
      <sequence first="{EXPRESSIONS}">
       <token first="{EXPRESSIONS}" name="EXPRESSIONS"/>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{ID}" name="LexerFileDeclaration">
   <action first="{ID}">
    <group first="{ID}">
     <sequence first="{ID}">
      <sequence first="{ID}">
       <nonterminal first="{ID}" ruleName="LexerKeyword"/>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
