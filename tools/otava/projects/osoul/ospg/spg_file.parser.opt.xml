<parserFile>
 <grammar name="SpgFileParser">
  <rule first="{PROJECT}" name="SpgFile">
   <action first="{PROJECT}">
    <group first="{PROJECT}">
     <sequence first="{PROJECT}">
      <sequence first="{PROJECT}">
       <sequence first="{PROJECT}">
        <token first="{PROJECT}" name="PROJECT"/>
        <action first="{ID}">
         <nonterminal first="{ID}" ruleName="QualifiedId"/>
        </action>
       </sequence>
       <token first="{SEMICOLON}" name="SEMICOLON"/>
      </sequence>
      <kleene first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}">
       <group first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}">
        <action first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}">
         <nonterminal first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}" ruleName="SpgFileDeclaration"/>
        </action>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}" name="SpgFileDeclaration">
   <choice first="{#, EXTERN, TOKENS, ID, SEMICOLON, FILEPATH}">
    <action first="{#, EXTERN, ID, SEMICOLON, FILEPATH}">
     <nonterminal first="{#, EXTERN, ID, SEMICOLON, FILEPATH}" ruleName="ParserFileDeclaration"/>
    </action>
    <action first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}">
     <nonterminal first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}" ruleName="TokenFileDeclaration"/>
    </action>
   </choice>
  </rule>
  <rule first="{#, EXTERN, ID, SEMICOLON, FILEPATH}" name="ParserFileDeclaration">
   <action first="{#, EXTERN, ID, SEMICOLON, FILEPATH}">
    <group first="{#, EXTERN, ID, SEMICOLON, FILEPATH}">
     <sequence first="{#, EXTERN, ID, SEMICOLON, FILEPATH}">
      <sequence first="{#, EXTERN, ID, FILEPATH}">
       <sequence first="{#, EXTERN, ID}">
        <group first="{#, EXTERN}">
         <choice first="{#, EXTERN}">
          <action first="{EXTERN}">
           <token first="{EXTERN}" name="EXTERN"/>
          </action>
          <action first="{#}">
           <empty first="{#}"/>
          </action>
         </choice>
        </group>
        <nonterminal first="{ID}" ruleName="ParserKeyword"/>
       </sequence>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}" name="TokenFileDeclaration">
   <action first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}">
    <group first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}">
     <sequence first="{#, EXTERN, TOKENS, SEMICOLON, FILEPATH}">
      <sequence first="{#, EXTERN, TOKENS, FILEPATH}">
       <sequence first="{#, EXTERN, TOKENS}">
        <group first="{#, EXTERN}">
         <choice first="{#, EXTERN}">
          <action first="{EXTERN}">
           <token first="{EXTERN}" name="EXTERN"/>
          </action>
          <action first="{#}">
           <empty first="{#}"/>
          </action>
         </choice>
        </group>
        <token first="{TOKENS}" name="TOKENS"/>
       </sequence>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
