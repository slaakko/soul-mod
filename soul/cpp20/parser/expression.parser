export module soul.cpp20.parser.expression;

[interface]import soul.cpp20.symbols.context;
[interface]import soul.cpp20.ast;
[implementation]import soul.cpp20.token;
[implementation]import soul.cpp20.lexer;
[implementation]import soul.cpp20.parser.attribute;
[implementation]import soul.cpp20.parser.initialization;
[implementation]import soul.cpp20.parser.type;
[implementation]import soul.cpp20.parser.function;
[implementation]import soul.cpp20.parser.literal;
[implementation]import soul.cpp20.parser.lambda;
[implementation]import soul.cpp20.parser.guard;
[implementation]import soul.cpp20.parser.concepts;
[implementation]import soul.cpp20.parser.identifier;
[implementation]import soul.cpp20.parser.declaration;
[implementation]import soul.cpp20.parser.punctuation;
[implementation]import soul.cpp20.parser.token;

parser ExpressionParser
{
    lexer soul::cpp20::lexer::Cpp20Lexer<char32_t>;

    using AttributeParser.AttributeSpecifierSeq;
    using InitializationParser.InitializerList;
    using InitializationParser.InitializerClause;
    using InitializationParser.BracedInitList;
    using InitializationParser.ExprOrBracedInitList;
    using TypeParser.TypeId;
    using TypeParser.TypeSpecifierSeq;
    using TypeParser.SimpleTypeSpecifierReset;
    using TypeParser.TypenameSpecifier;
    using FunctionParser.ParameterDeclarationClause;
    using LiteralParser.Literal;
    using LambdaParser.LambdaExpression;
    using GuardParser.ConceptGuard;
    using ConceptParser.RequiresExpression;
    using IdentifierParser.QualifiedId;
    using IdentifierParser.UnqualifiedId;
    using DeclarationParser.PtrOperator;
    using PunctuationParser.Colon;
    using PunctuationParser.Quest;
    using PunctuationParser.Ellipsis;
    using PunctuationParser.LBracket;
    using PunctuationParser.RBracket;

    Expression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, 
        var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos commaPos) : soul::cpp20::ast::Node*
        ::= 
        (
            AssignmentExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (   
                COMMA{ commaPos = lexer.GetSourcePos(pos); } AssignmentExpression(context):right
                {
                    expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::CommaNode(commaPos), expr.release(), right));
                }
            )*
        )
        {
            return expr.release();
        }
        ;

    ConstantExpression(soul::cpp20::symbols::Context* context) : soul::cpp20::ast::Node*
        ::= ConditionalExpression(context):conditionalExpr{ return conditionalExpr; }
        ;

    AssignmentExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (   YieldExpression(context):yieldExpr{ expr.reset(yieldExpr); }
        |   ThrowExpression(context):throwExpr{ expr.reset(throwExpr); }
        |   (LogicalOrExpression(context):left{ sourcePos = lexer.GetSourcePos(pos); } AssignmentOp:op InitializerClause(context):right)
            {
                expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, left, right));
            }
        |   ConditionalExpression(context):conditionalExpr{ expr.reset(conditionalExpr); }
        )
        {
            return expr.release();
        }
        ;

    AssignmentOp : soul::cpp20::ast::Node*
        ::= ASSIGN{ return new soul::cpp20::ast::AssignNode(lexer.GetSourcePos(pos)); }
        |   MUL_ASSIGN{ return new soul::cpp20::ast::MulAssignNode(lexer.GetSourcePos(pos)); }
        |   DIV_ASSIGN{ return new soul::cpp20::ast::DivAssignNode(lexer.GetSourcePos(pos)); }
        |   REM_ASSIGN{ return new soul::cpp20::ast::ModAssignNode(lexer.GetSourcePos(pos)); }
        |   ADD_ASSIGN{ return new soul::cpp20::ast::PlusAssignNode(lexer.GetSourcePos(pos)); }
        |   SUB_ASSIGN{ return new soul::cpp20::ast::MinusAssignNode(lexer.GetSourcePos(pos)); }
        |   SHIFT_RIGHT_ASSIGN{ return new soul::cpp20::ast::ShiftRightAssignNode(lexer.GetSourcePos(pos)); }
        |   SHIFT_LEFT_ASSIGN{ return new soul::cpp20::ast::ShiftLeftAssignNode(lexer.GetSourcePos(pos)); }
        |   AND_ASSIGN{ return new soul::cpp20::ast::AndAssignNode(lexer.GetSourcePos(pos)); }
        |   XOR_ASSIGN{ return new soul::cpp20::ast::XorAssignNode(lexer.GetSourcePos(pos)); }
        |   OR_ASSIGN{ return new soul::cpp20::ast::OrAssignNode(lexer.GetSourcePos(pos)); }
        ;

    ConditionalExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (   LogicalOrExpression(context):cond{ sourcePos = lexer.GetSourcePos(pos); expr.reset(cond); } 
            (
                Quest:quest Expression(context):thenExpr Colon:colon AssignmentExpression(context):elseExpr
                {
                    expr.reset(new soul::cpp20::ast::ConditionalExprNode(sourcePos, expr.release(), quest, thenExpr, colon, elseExpr));
                }
            )?
        )
        {
            return expr.release();
        }
        ;

    YieldExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= CO_YIELD{ sourcePos = lexer.GetSourcePos(pos); }
        (   AssignmentExpression(context):assignmentExpr{ return new soul::cpp20::ast::YieldExprNode(sourcePos, assignmentExpr); }
        |   BracedInitList(context):bracedInitList{ return new soul::cpp20::ast::YieldExprNode(sourcePos, bracedInitList); }
        )
        ;

    ThrowExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            THROW{ sourcePos = lexer.GetSourcePos(pos); } AssignmentExpression(context):exception?
        )
        {
            return new soul::cpp20::ast::ThrowExprNode(sourcePos, exception);
        }
        ;

    LogicalOrExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::= 
        (
            LogicalAndExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (
                OR_OR{ opPos = lexer.GetSourcePos(pos); } 
                LogicalAndExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::DisjunctionNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    LogicalAndExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::= 
        (
            InclusiveOrExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (
                AMP_AMP{ opPos = lexer.GetSourcePos(pos); } 
                InclusiveOrExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::ConjunctionNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    InclusiveOrExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::= 
        (
            ExclusiveOrExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (
                OR{ opPos = lexer.GetSourcePos(pos); } 
                ExclusiveOrExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::InclusiveOrNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    ExclusiveOrExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::= 
        (
            AndExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (
                XOR{ opPos = lexer.GetSourcePos(pos); } 
                AndExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::ExclusiveOrNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    AndExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::= 
        (
            EqualityExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); } 
            (
                AMP{ opPos = lexer.GetSourcePos(pos); } 
                EqualityExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::AndNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    EqualityExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            RelationalExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                EqualityOp:op RelationalExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    EqualityOp : soul::cpp20::ast::Node*
        ::= EQ{ return new soul::cpp20::ast::EqualNode(lexer.GetSourcePos(pos)); }
        |   NEQ{ return new soul::cpp20::ast::NotEqualNode(lexer.GetSourcePos(pos)); }
        ;


    RelationalExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            CompareExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                RelationalOp(context->GetFlag(soul::cpp20::symbols::ContextFlags::parsingTemplateId)):op 
                CompareExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    RelationalOp(bool rejectRAngle) : soul::cpp20::ast::Node*
        ::= LANGLE{ return new soul::cpp20::ast::LessNode(lexer.GetSourcePos(pos)); }
        |   empty{ pass = !rejectRAngle; } RANGLE{ return new soul::cpp20::ast::GreaterNode(lexer.GetSourcePos(pos)); }
        |   LEQ{ return new soul::cpp20::ast::LessOrEqualNode(lexer.GetSourcePos(pos)); }
        |   GEQ{ return new soul::cpp20::ast::GreaterOrEqualNode(lexer.GetSourcePos(pos)); }
        ;

    CompareExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos opPos) : soul::cpp20::ast::Node*
        ::=
        (
            ShiftExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                SPACE_SHIP{ opPos = lexer.GetSourcePos(pos); } 
                ShiftExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, new soul::cpp20::ast::CompareNode(opPos), expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    ShiftExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            AdditiveExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                ShiftOp:op 
                AdditiveExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    ShiftOp : soul::cpp20::ast::Node*
        ::= SHIFT_LEFT{ return new soul::cpp20::ast::ShiftLeftNode(lexer.GetSourcePos(pos)); }
        |   SHIFT_RIGHT{ return new soul::cpp20::ast::ShiftRightNode(lexer.GetSourcePos(pos)); }
        ;

    AdditiveExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            MultiplicativeExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                AdditiveOp:op 
                MultiplicativeExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    AdditiveOp : soul::cpp20::ast::Node*
        ::= PLUS{ return new soul::cpp20::ast::PlusNode(lexer.GetSourcePos(pos)); }
        |   MINUS{ return new soul::cpp20::ast::MinusNode(lexer.GetSourcePos(pos)); }
        ;

    MultiplicativeExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : 
        soul::cpp20::ast::Node*
        ::= 
        (
            PmExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                MultiplicativeOp:op 
                PmExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    MultiplicativeOp : soul::cpp20::ast::Node*
        ::= STAR{ return new soul::cpp20::ast::MulNode(lexer.GetSourcePos(pos)); }
        |   DIV{ return new soul::cpp20::ast::DivNode(lexer.GetSourcePos(pos)); }
        |   MOD{ return new soul::cpp20::ast::ModNode(lexer.GetSourcePos(pos)); }
        ;

    PmExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (
            CastExpression(context):left{ expr.reset(left); sourcePos = lexer.GetSourcePos(pos); }
            (
                PmOp:op 
                CastExpression(context):right{ expr.reset(new soul::cpp20::ast::BinaryExprNode(sourcePos, op, expr.release(), right)); }
            )*
        )
        {
            return expr.release();
        }
        ;

    PmOp : soul::cpp20::ast::Node*
        ::= DOT_STAR{ return new soul::cpp20::ast::DotStarNode(lexer.GetSourcePos(pos)); }
        |   ARROW_STAR{ return new soul::cpp20::ast::ArrowStarNode(lexer.GetSourcePos(pos)); }
        ;

    CastExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos, 
        var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : soul::cpp20::ast::Node*
        ::=  
        (
            (   
                (
                    LPAREN{ sourcePos = lexer.GetSourcePos(pos); lpPos = sourcePos; } 
                    TypeId(context):typeId 
                    RPAREN{ rpPos = lexer.GetSourcePos(pos); } 
                    CastExpression(context):child
                )
                {
                    expr.reset(new soul::cpp20::ast::CastExprNode(sourcePos, typeId, child, lpPos, rpPos));
                }
            )
            |   UnaryExpression(context):ue{ expr.reset(ue); }
        )
        {
            return expr.release();
        }
        ;

    UnaryExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> expr, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= 
        (   UnaryOp:op{ sourcePos = lexer.GetSourcePos(pos); } CastExpression(context):child{ expr.reset(new soul::cpp20::ast::UnaryExprNode(sourcePos, op, child)); }
        |   AwaitExpression(context):awaitExpr{ expr.reset(awaitExpr); }
        |   SizeOfExpression(context):sizeOfExpr{ expr.reset(sizeOfExpr); }
        |   AlignOfExpression(context):alignOfExpr{ expr.reset(alignOfExpr); }
        |   NoexceptEpression(context):noexceptExpr{ expr.reset(noexceptExpr); }
        |   NewExpression(context):newExpr{ expr.reset(newExpr); }
        |   DeleteExpression(context):deleteExpr{ expr.reset(deleteExpr); }
        |   PostfixExpression(context):postfixExpr{ expr.reset(postfixExpr); }
        )
        {
            return expr.release();
        }
        ;

    UnaryOp : soul::cpp20::ast::Node*
        ::= STAR{ return new soul::cpp20::ast::DerefNode(lexer.GetSourcePos(pos)); }
        |   AMP{ return new soul::cpp20::ast::AddrOfNode(lexer.GetSourcePos(pos)); }
        |   PLUS{ return new soul::cpp20::ast::PlusNode(lexer.GetSourcePos(pos)); }
        |   MINUS{ return new soul::cpp20::ast::MinusNode(lexer.GetSourcePos(pos)); }
        |   EXCLAMATION{ return new soul::cpp20::ast::NotNode(lexer.GetSourcePos(pos)); }
        |   TILDE{ return new soul::cpp20::ast::ComplementNode(lexer.GetSourcePos(pos)); }
        |   PLUS_PLUS{ return new soul::cpp20::ast::PrefixIncNode(lexer.GetSourcePos(pos)); }
        |   MINUS_MINUS{ return new soul::cpp20::ast::PrefixDecNode(lexer.GetSourcePos(pos)); }
        ;

    AwaitExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos) : soul::cpp20::ast::Node*
        ::= CO_AWAIT{ sourcePos = lexer.GetSourcePos(pos); } CastExpression(context):child{ return new soul::cpp20::ast::AwaitExprNode(sourcePos, child); }
        ;

    SizeOfExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos, 
        var soul::ast::SourcePos ellipsisPos, var std::unique_ptr<soul::cpp20::ast::Node> childNode) : soul::cpp20::ast::Node*
        ::= SIZEOF{ sourcePos = lexer.GetSourcePos(pos); } LPAREN{ lpPos = lexer.GetSourcePos(pos); } TypeId(context):typeId RPAREN
            { 
                rpPos = lexer.GetSourcePos(pos); 
                return new soul::cpp20::ast::SizeOfTypeExprNode(sourcePos, typeId, lpPos, rpPos); 
            }
        |   SIZEOF{ sourcePos = lexer.GetSourcePos(pos); } ELLIPSIS{ ellipsisPos = lexer.GetSourcePos(pos); } 
            LPAREN{ lpPos = lexer.GetSourcePos(pos); } 
            ID{ childNode.reset(soul::cpp20::parser::token::ParseIdentifier(lexer.GetSourcePos(pos), lexer.FileName(), lexer.GetToken(pos))); } 
            RPAREN
            { 
                rpPos = lexer.GetSourcePos(pos); 
                return new soul::cpp20::ast::SizeOfPackExprNode(sourcePos, childNode.release(), ellipsisPos, lpPos, rpPos); 
            }
        |   SIZEOF{ sourcePos = lexer.GetSourcePos(pos); } UnaryExpression(context):child{ return new soul::cpp20::ast::SizeOfUnaryExprNode(sourcePos, child); }
        ;

    AlignOfExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : 
        soul::cpp20::ast::Node*
        ::= 
        (
            ALIGNOF{ sourcePos = lexer.GetSourcePos(pos); } LPAREN{ lpPos = lexer.GetSourcePos(pos); } TypeId(context):typeId RPAREN{ rpPos = lexer.GetSourcePos(pos); }
        )
        {
            return new soul::cpp20::ast::AlignOfExprNode(sourcePos, typeId, lpPos, rpPos);
        }
        ;

    NoexceptEpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : 
        soul::cpp20::ast::Node*
        ::= 
        (
            NOEXCEPT{ sourcePos = lexer.GetSourcePos(pos); } LPAREN{ lpPos = lexer.GetSourcePos(pos); } Expression(context):expr RPAREN{ rpPos = lexer.GetSourcePos(pos); }
        )
        {
            return new soul::cpp20::ast::NoexceptExprNode(sourcePos, expr, lpPos, rpPos);
        }
        ;

    NewExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var std::unique_ptr<soul::cpp20::ast::Node> colonColonNode, 
        var soul::ast::SourcePos newPos) : soul::cpp20::ast::Node*
        ::= 
        (
            (   COLON_COLON{ sourcePos = lexer.GetSourcePos(pos); colonColonNode.reset(new soul::cpp20::ast::ColonColonNode(sourcePos)); } NEW{ newPos = lexer.GetSourcePos(pos); }
            |   NEW{ sourcePos = lexer.GetSourcePos(pos); newPos = sourcePos; }
            )
            NewPlacement(context):placement? NewTypeId(context):newTypeId NewInitializer(context):newInitializer?
        )
        {
            return new soul::cpp20::ast::NewExprNode(sourcePos, placement, newTypeId, newInitializer, colonColonNode.release(), newPos);
        }
        ;

    NewPlacement(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::NewPlacementNode> placement) : soul::cpp20::ast::Node*
        ::= 
        (
            LPAREN{ soul::ast::SourcePos sourcePos = lexer.GetSourcePos(pos); placement.reset(new soul::cpp20::ast::NewPlacementNode(sourcePos)); placement->SetLParenPos(sourcePos); } 
            ExpressionList(context, placement.get()):el 
            RPAREN{ placement->SetRParenPos(lexer.GetSourcePos(pos)); }
        )
        {
            return placement.release();
        }
        ;

    NewInitializer(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::NewInitializerNode> node, 
        var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : soul::cpp20::ast::Node*
        ::= 
        (
            (
                LPAREN{ lpPos = lexer.GetSourcePos(pos); node.reset(new soul::cpp20::ast::NewInitializerNode(lpPos)); } 
                ExpressionList(context, node.get()):exprList? 
                RPAREN{ rpPos = lexer.GetSourcePos(pos); }
            )
            {
                node->SetLParenPos(lpPos);
                node->SetRParenPos(rpPos);
                return node.release();
            }
        |   BracedInitList(context):bracedInitList{ return bracedInitList; }
        )
        ;

    NewTypeId(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : 
        soul::cpp20::ast::Node*
        ::= 
        (
            LPAREN{ sourcePos = lexer.GetSourcePos(pos); lpPos = sourcePos; } 
            TypeId(context):typeId 
            RPAREN{ rpPos = lexer.GetSourcePos(pos); }
        )
        { 
            return new soul::cpp20::ast::ParenNewTypeIdNode(sourcePos, typeId, lpPos, rpPos); 
        }
        |   empty{ context->PushResetFlag(soul::cpp20::symbols::ContextFlags::hasDefiningTypeSpecifier); }
            TypeSpecifierSeq(context):typeSpecifierSeq{ sourcePos = lexer.GetSourcePos(pos); context->PopFlags(); } / { context->PopFlags(); }
            (   NewDeclarator(context):newDeclarator{ return new soul::cpp20::ast::NewTypeIdNode(sourcePos, typeSpecifierSeq, newDeclarator); }
            |   empty{ return new soul::cpp20::ast::NewTypeIdNode(sourcePos, typeSpecifierSeq, nullptr); }
            )
        ;

    NewDeclarator(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> node) : soul::cpp20::ast::Node*
        ::= 
        (
            PtrOperator(context):first{ node.reset(new soul::cpp20::ast::NewDeclaratorNode(lexer.GetSourcePos(pos))); node->AddNode(first); } 
            (
                PtrOperator(context):next{ node->AddNode(next); }
            )* 
            (
                NoPtrNewDeclarator(context):noPtrNewDeclarator{ node->AddNode(noPtrNewDeclarator); }
            |   empty
            )
        |   NoPtrNewDeclarator(context):noPtrNewDecl{ node.reset(noPtrNewDecl); }
        )
        {
            return node.release();
        }
        ;

    NoPtrNewDeclarator(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> node) : soul::cpp20::ast::Node*
        ::= 
        (
            (
                LBracket:lb{ node.reset(new soul::cpp20::ast::ArrayNewDeclaratorNode(lexer.GetSourcePos(pos))); node->AddNode(lb); } 
                (Expression(context):size{ node->AddNode(size); })? 
                RBracket:rb{ node->AddNode(rb); } 
            )
            (
                LBracket:lb2{ node->AddNode(lb2); } 
                ConstantExpression(context):size2{ node->AddNode(size2); }
                RBracket:rb2{ node->AddNode(rb2); } 
            )*
            AttributeSpecifierSeq(context):attributes?
        )
        {
            return node.release();
        }
        ;

    DeleteExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var std::unique_ptr<soul::cpp20::ast::Node> colonColonNode, 
