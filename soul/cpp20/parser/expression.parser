                |   MINUS_MINUS{ expr.reset(new soul::cpp20::ast::PostfixDecExprNode(sourcePos, expr.release(), lexer.GetSourcePos(pos))); }
                )*
            )
        )
        {
            return expr.release();
        }
        ;

    CppCastExpression(soul::cpp20::symbols::Context* context, var std::unique_ptr<soul::cpp20::ast::Node> op, 
        var soul::ast::SourcePos laPos, var soul::ast::SourcePos raPos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos) : soul::cpp20::ast::Node*
        ::= 
        (
            (   DYNAMIC_CAST{ op.reset(new soul::cpp20::ast::DynamicCastNode(lexer.GetSourcePos(pos))); }
