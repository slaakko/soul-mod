    TypeIdExpression(soul::cpp20::symbols::Context* context, var soul::ast::SourcePos sourcePos, var soul::ast::SourcePos lpPos, var soul::ast::SourcePos rpPos, 
        var std::unique_ptr<soul::cpp20::ast::Node> child) : soul::cpp20::ast::Node*
        ::= 
