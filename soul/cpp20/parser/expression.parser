            (   CppCastExpression(context):cppCastExpr{ sourcePos = lexer.GetSourcePos(pos); expr.reset(cppCastExpr); }
            |   TypeIdExpression(context):typeIdExpr{ sourcePos = lexer.GetSourcePos(pos); expr.reset(typeIdExpr); }
