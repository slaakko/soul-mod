<parserFile>
 <grammar name="InitializationParser">
  <rule first="{}" name="Initializer">
   <action first="{}">
    <group first="{}">
     <choice first="{}">
      <action first="{}">
       <nonterminal first="{}" ruleName="BraceOrEqualInitializer"/>
      </action>
      <sequence first="{}">
       <sequence first="{}">
        <action first="{}">
         <token first="{}" name="LPAREN"/>
        </action>
        <nonterminal first="{}" ruleName="ExpressionList"/>
       </sequence>
       <action first="{}">
        <token first="{}" name="RPAREN"/>
       </action>
      </sequence>
     </choice>
    </group>
   </action>
  </rule>
  <rule first="{}" name="InitializerClause">
   <choice first="{}">
    <action first="{}">
     <nonterminal first="{}" ruleName="BracedInitList"/>
    </action>
    <action first="{}">
     <nonterminal first="{}" ruleName="AssignmentExpression"/>
    </action>
   </choice>
  </rule>
  <rule first="{}" name="InitializerList">
   <group first="{}">
    <sequence first="{}">
     <sequence first="{}">
      <action first="{}">
       <nonterminal first="{}" ruleName="InitializerClause"/>
      </action>
      <kleene first="{}">
       <group first="{}">
        <sequence first="{}">
         <action first="{}">
          <nonterminal first="{}" ruleName="Comma"/>
         </action>
         <action first="{}">
          <nonterminal first="{}" ruleName="InitializerClause"/>
         </action>
        </sequence>
       </group>
      </kleene>
     </sequence>
     <optional first="{}">
      <group first="{}">
       <action first="{}">
        <nonterminal first="{}" ruleName="Ellipsis"/>
       </action>
      </group>
     </optional>
    </sequence>
   </group>
  </rule>
  <rule first="{}" name="DesignatedInitializerList">
   <group first="{}">
    <sequence first="{}">
     <action first="{}">
      <nonterminal first="{}" ruleName="DesignatedInitializerClause"/>
     </action>
     <kleene first="{}">
      <group first="{}">
       <sequence first="{}">
        <action first="{}">
         <nonterminal first="{}" ruleName="Comma"/>
        </action>
        <action first="{}">
         <nonterminal first="{}" ruleName="DesignatedInitializerClause"/>
        </action>
       </sequence>
      </group>
     </kleene>
    </sequence>
   </group>
  </rule>
  <rule first="{}" name="DesignatedInitializerClause">
   <action first="{}">
    <group first="{}">
     <sequence first="{}">
      <action first="{}">
       <nonterminal first="{}" ruleName="Designator"/>
      </action>
      <nonterminal first="{}" ruleName="BraceOrEqualInitializer"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{}" name="Designator">
   <action first="{}">
    <group first="{}">
     <sequence first="{}">
      <action first="{}">
       <token first="{}" name="DOT"/>
      </action>
      <nonterminal first="{}" ruleName="Identifier"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{}" name="ExprOrBracedInitList">
   <choice first="{}">
    <action first="{}">
     <nonterminal first="{}" ruleName="BracedInitList"/>
    </action>
    <action first="{}">
     <nonterminal first="{}" ruleName="Expression"/>
    </action>
   </choice>
  </rule>
  <rule first="{}" name="BraceOrEqualInitializer">
   <choice first="{}">
    <sequence first="{}">
     <action first="{}">
      <token first="{}" name="ASSIGN"/>
     </action>
     <action first="{}">
      <nonterminal first="{}" ruleName="InitializerClause"/>
     </action>
    </sequence>
    <action first="{}">
     <nonterminal first="{}" ruleName="BracedInitList"/>
    </action>
   </choice>
  </rule>
  <rule first="{}" name="BracedInitList">
   <action first="{}">
    <group first="{}">
     <choice first="{}">
      <sequence first="{}">
       <sequence first="{}">
        <sequence first="{}">
         <action first="{}">
          <nonterminal first="{}" ruleName="LBrace"/>
         </action>
         <group first="{}">
          <choice first="{}">
           <nonterminal first="{}" ruleName="InitializerList"/>
           <nonterminal first="{}" ruleName="DesignatedInitializerList"/>
          </choice>
         </group>
        </sequence>
        <optional first="{}">
         <group first="{}">
          <action first="{}">
           <nonterminal first="{}" ruleName="Comma"/>
          </action>
         </group>
        </optional>
       </sequence>
       <action first="{}">
        <nonterminal first="{}" ruleName="RBrace"/>
       </action>
      </sequence>
      <sequence first="{}">
       <action first="{}">
        <nonterminal first="{}" ruleName="LBrace"/>
       </action>
       <action first="{}">
        <nonterminal first="{}" ruleName="RBrace"/>
       </action>
      </sequence>
     </choice>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
