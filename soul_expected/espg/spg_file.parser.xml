<parserFile>
 <grammar name="SpgFileParser">
  <rule first="{PROJECT}" name="SpgFile">
   <action first="{PROJECT}">
    <group first="{PROJECT}">
     <sequence first="{PROJECT}">
      <sequence first="{PROJECT}">
       <sequence first="{PROJECT}">
        <token first="{PROJECT}" name="PROJECT"/>
        <action first="{ID}">
         <nonterminal first="{ID}" ruleName="QualifiedId"/>
        </action>
       </sequence>
       <token first="{SEMICOLON}" name="SEMICOLON"/>
      </sequence>
      <kleene first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}">
       <group first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}">
        <action first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}">
         <nonterminal first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}" ruleName="SpgFileDeclaration"/>
        </action>
       </group>
      </kleene>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}" name="SpgFileDeclaration">
   <choice first="{SEMICOLON, ID, EXTERN, #, TOKENS, FILEPATH}">
    <action first="{SEMICOLON, ID, EXTERN, #, FILEPATH}">
     <nonterminal first="{SEMICOLON, ID, EXTERN, #, FILEPATH}" ruleName="ParserFileDeclaration"/>
    </action>
    <action first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}">
     <nonterminal first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}" ruleName="TokenFileDeclaration"/>
    </action>
   </choice>
  </rule>
  <rule first="{SEMICOLON, ID, EXTERN, #, FILEPATH}" name="ParserFileDeclaration">
   <action first="{SEMICOLON, ID, EXTERN, #, FILEPATH}">
    <group first="{SEMICOLON, ID, EXTERN, #, FILEPATH}">
     <sequence first="{SEMICOLON, ID, EXTERN, #, FILEPATH}">
      <sequence first="{ID, EXTERN, #, FILEPATH}">
       <sequence first="{ID, EXTERN, #}">
        <group first="{EXTERN, #}">
         <choice first="{EXTERN, #}">
          <action first="{EXTERN}">
           <token first="{EXTERN}" name="EXTERN"/>
          </action>
          <action first="{#}">
           <empty first="{#}"/>
          </action>
         </choice>
        </group>
        <nonterminal first="{ID}" ruleName="ParserKeyword"/>
       </sequence>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
  <rule first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}" name="TokenFileDeclaration">
   <action first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}">
    <group first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}">
     <sequence first="{SEMICOLON, EXTERN, #, TOKENS, FILEPATH}">
      <sequence first="{EXTERN, #, TOKENS, FILEPATH}">
       <sequence first="{EXTERN, #, TOKENS}">
        <group first="{EXTERN, #}">
         <choice first="{EXTERN, #}">
          <action first="{EXTERN}">
           <token first="{EXTERN}" name="EXTERN"/>
          </action>
          <action first="{#}">
           <empty first="{#}"/>
          </action>
         </choice>
        </group>
        <token first="{TOKENS}" name="TOKENS"/>
       </sequence>
       <nonterminal first="{FILEPATH}" ruleName="FilePath"/>
      </sequence>
      <token first="{SEMICOLON}" name="SEMICOLON"/>
     </sequence>
    </group>
   </action>
  </rule>
 </grammar>
</parserFile>
